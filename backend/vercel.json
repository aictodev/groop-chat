{
    "version": 2,
    "functions": {
        "index.js": {
            "includeFiles": "prompts/**",
            "maxDuration": 60
        },
        "debug.js": {
            "maxDuration": 10
        }
    },
    "routes": [
        {
            "src": "/debug",
            "dest": "/debug.js"
        },
        {
            "src": "/(.*)",
            "dest": "/index.js"
        }
    ]
}